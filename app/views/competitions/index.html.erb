      <tr>
        <th scope="col" class="px-6 py-4 font-medium text-gray-900"><%= Competition.human_attribute_name("date") %></th>
        <th scope="col" class="px-6 py-4 font-medium text-gray-900"><%= Competition.human_attribute_name("name") %></th>
        <th scope="col" class="px-6 py-4 font-medium text-gray-900"></th>
      </tr>
    </thead>
<% if @competitions.present? %>
